<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>mvn02</display-name>
  <welcome-file-list>
    <welcome-file>index.bit</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>index</servlet-name>
  	<servlet-class>com.bit.day16.controller.IndexController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>intro</servlet-name>
  	<servlet-class>com.bit.day16.controller.IntroController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>list</servlet-name>
  	<servlet-class>com.bit.day16.controller.ListController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>com.bit.day16.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>one</servlet-name>
  	<servlet-class>com.bit.day16.controller.OneController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>del</servlet-name>
  	<servlet-class>com.bit.day16.controller.DeleteController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>index</servlet-name>
  	<url-pattern>/index.bit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>intro</servlet-name>
  	<url-pattern>/intro.bit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>list</servlet-name>
  	<url-pattern>/list.bit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login.bit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>one</servlet-name>
  	<url-pattern>/select.bit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>del</servlet-name>
  	<url-pattern>/delete.bit</url-pattern>
  </servlet-mapping>
</web-app>











